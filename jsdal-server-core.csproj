<Project Sdk="Microsoft.NET.Sdk.Web">
	<PropertyGroup>
		<AssemblyName>jsdal-server</AssemblyName>
		<TargetFramework>netcoreapp3.0</TargetFramework>
		<RuntimeIdentifier>win7-x64</RuntimeIdentifier>
		<PlatformTarget>AnyCPU</PlatformTarget>
		<!-- <OutputType>Exe</OutputType> -->
		<TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
 
		<SelfContained>false</SelfContained>
		<IsTransformWebConfigDisabled>true</IsTransformWebConfigDisabled>        

	</PropertyGroup>
	<ItemGroup>
		<Folder Include="wwwroot/" />
		
     
  <Content Include=".\plugins\**\*.*" CopyToOutputDirectory="Always" CopyToPublishDirectory="Always" />

		<Content Remove="00-Release/**/*" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="./../jsdal-plugin/jsdal-plugin.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="MirrorSharp.Common">
			<HintPath>Refs\MirrorSharp.Common.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ItemGroup>
		<None Include="appsettings.json" CopyToPublishDirectory="PreserveNewest" CopyToOutputDirectory="PreserveNewest" />
	</ItemGroup>

	<ItemGroup>

