using jsdal_plugin;

[PluginData("Server-method test","1E8F13C2-D326-4C9E-94A1-DB2E4F40588D", "Some basic tests with different return types?")]
class SomeServerMethodTests : ServerMethodPlugin
{
    public SomeServerMethodTests()
    {
	
    }
    
    [ServerMethod]
    public static string GetServerTime()
    {
    	return System.DateTime.Now.ToString("yyyyMMdd HH:mm");
    }
     
}
